{
  "name": "react-front-end",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:development": "env-cmd -f ./.env.development npm run-script start",
    "start:production": "env-cmd -f ./.env.production npm run-script start",
    "build:development": "env-cmd -f ./.env.development npm run-script build",
    "build:production": "env-cmd -f ./.env.production npm run-script build"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "axios": "^0.21.0",
    "env-cmd": "^10.1.0",
    "query-string": "^6.13.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-xml-parser": "^1.1.8",
    "styled-components": "^5.2.1",
    "web-vitals": "^0.2.4",
    "xml-formatter": "^2.4.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^16.6.1",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.12",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "typescript": "~4.0.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
