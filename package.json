{
  "name": "marcpresentation-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test:cypress": "REACT_APP_USE_MOCK=true start-server-and-test http://localhost:3000",
    "test": "cypress run",
    "start:development": "env-cmd -f ./.env.development npm run-script start",
    "start:production": "env-cmd -f ./.env.production npm run-script start"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "env-cmd": "^10.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/instrument-cra": "^1.4.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.22",
    "axios-mock-adapter": "^1.20.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "cypress": "^9.4.1",
    "eslint-plugin-cypress": "^2.12.1",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "typescript": "~4.5.5",
    "start-server-and-test": "^1.14.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
