{
  "name": "marcpresentation-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "start:development": "env-cmd -f ./.env.development npm run-script start",
    "start:production": "env-cmd -f ./.env.production npm run-script start",
    "test": "cypress run",
    "test:cypress": "REACT_APP_USE_MOCK=true start-server-and-test http://localhost:3000"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "axios": "^0.27.2",
    "env-cmd": "^10.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/instrument-cra": "^1.4.0",
    "@types/node": "^17.0.31",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.2",
    "@types/styled-components": "^5.1.25",
    "axios-mock-adapter": "^1.20.0",
    "cypress": "^9.6.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "start-server-and-test": "^1.14.0",
    "typescript": "~4.6.4"
  }
}
